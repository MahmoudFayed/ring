# 			OOP - Functional 

new Program {
		1 2 3
		4 5 6
		7 8 9 
}

class Program
	aList = []

func BraceExprEval value
	if isNumber(value) 
		aList + value
	ok 
	
func BraceEnd 
	nSum = 0 
	for nNumber in aList
		nSum += nNumber
	next
	? "Sum :" + nSum
